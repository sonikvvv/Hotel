            "mainClass": "logic.DecodeOperation",
            "projectName": "bussines-logic"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<graphical-user-interface>",
            "request": "launch",
            "mainClass": "gui.App",
            "projectName": "graphical-user-interface"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-testAPP<graphical-user-interface>",
            "request": "launch",
            "mainClass": "gui.references.testAPP",
            "projectName": "graphical-user-interface"
        }


    ]
}